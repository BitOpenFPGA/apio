{
  "iceprog": {
    "command": "iceprog",
    "args": "-d i:0x${VID}:0x${PID}:${FTDI_ID}"
  },
  "icoprog": {
    "command": "export WIRINGPI_GPIOMEM=1; icoprog",
    "args": "-p <"
  },
  "litterbox": {
    "command": "sudo litterbox",
    "args": "-c"
  },
  "blackiceprog": {
    "command": "black-iceprog",
    "args": "${DEVICE}"
  },
  "tinyfpgab": {
    "command": "tinyfpgab",
    "args": "-c ${DEVICE} --program "
  }
}
