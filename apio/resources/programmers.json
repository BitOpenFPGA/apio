{
  "iceprog": {
    "command": "iceprog",
    "args": "-d i:0x0403:0x6010:%D%"
  },
  "icoprog": {
    "command": "export WIRINGPI_GPIOMEM=1; icoprog",
    "args": "-p <"
  },
  "litterbox": {
    "command": "sudo litterbox",
    "args": "-c"
  },
  "blackiceprog": {
    "command": "black-iceprog",
    "args": "%D% "
  },
  "tinyfpgab": {
    "command": "tinyfpgab",
    "args": "-c %D% --program "
  }
}
